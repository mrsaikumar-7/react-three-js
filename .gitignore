# Ignore node_modules in both client and backend
/client/node_modules
/backend/node_modules

# Ignore build artifacts
/client/build
/backend/build

# Ignore log files
/client/logs
/backend/logs

# Ignore IDE files
/client/.idea
/backend/.idea
/client/.vscode
/backend/.vscode

# Ignore editor configuration
/client/*.suo
/backend/*.suo
/client/*.ntvs*
/backend/*.ntvs*
/client/*.njsproj
/backend/*.njsproj
/client/*.sln
/backend/*.sln

# Ignore environment variables
/client/.env
/backend/.env

# Ignore OS-generated files
/client/.DS_Store
/backend/.DS_Store
/client/Thumbs.db
/backend/Thumbs.db
